public class Solution {
    public void sortColors(int[] nums) {
    if(nums==null){
        return;
    }    
    int p0=0,p1=nums.length-1,index=0;
    while(index<=p1){
        if(nums[index]==0){
            nums[index]=nums[p0];
            nums[p0]=0;
            p0++;
            
        }
        if(nums[index]==2){
            nums[index]=nums[p1];
            nums[p1]=2;
            p1--;
            index--;
        }
        index++;
    }
    }
}