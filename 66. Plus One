public class Solution {
    public int[] plusOne(int[] digits) {
        if(digits==null||digits.length==0){
            int[] res={1};
            return res;
        }
        for(int i=digits.length-1;i>=0;--i){
            if(digits[i]==9){
                digits[i]=0;
                //return digits;
            }
            else{
                digits[i]+=1;
                return digits;
            }
        }
        int[] res=new int[digits.length+1];
        res[0]=1;
      
        return res;
    }
}